{
  "manifest_version": 2,

  "name": "YouTube Profile Photo Viewer",
  "description": "YouTube Profile Photo Viewer for channels.",
  "version": "1.0.0",
  "icons": { "128": "icon_1.png" },
  "content_scripts": [{

      "css" : ["inject_main.css"],
      "js": ["content.js"],
      "matches" : [
        "https://www.youtube.com/*"
      ]
    }],
  "browser_action": {
    "default_icon": "icon_1.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
