{
  "manifest_version": 2,

  "name": "YouTube Profile Photo Viewer",
  "description": "YouTube Profile Photo Viewer",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "content_scripts": [
    {
      "matches" : [
        "https://youtube.com/",
        "https://www.youtube.com/c/AngularFirebase",
        "https://atharva.works/"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
